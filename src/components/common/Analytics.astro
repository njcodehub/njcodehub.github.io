---
const gaId = 'G-CWRZLDKN40';
---

{
  gaId && (
    <>
      <script is:inline define:vars={{ gaId }} async src={'https://www.googletagmanager.com/gtag/js?id=' + gaId} />
      <script is:inline define:vars={{ gaId }}>
        window.dataLayer = window.dataLayer || []; const gtag = (...args) => dataLayer.push(...args); gtag('js', new
        Date()); gtag('config', gaId);
      </script>
    </>
  )
}
